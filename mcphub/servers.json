{
  "mcpServers": {
    "github": {
      "disabled": false,
      "headers": {
        "Authorization": "Bearer ${MCPHUB_GITHUB_TOKEN}"
      },
      "url": "https://api.githubcopilot.com/mcp/",
      "autoApprove": [
        "get_me",
        "list_pull_requests",
        "get_pull_request",
        "get_pull_request_comments",
        "list_issues",
        "get_issue",
        "get_issue_comments"
      ]
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "sequentialthinking": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "mcp/sequentialthinking"]
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    }
  },
  "nativeMCPServers": {}
}

{
  "mcpServers": {
    "github": {
      "disabled": false,
      "headers": {
        "Authorization": "Bearer ${MCPHUB_GITHUB_TOKEN}"
      },
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "sequentialthinking": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "mcp/sequentialthinking"]
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    }
  },
  "nativeMCPServers": {}
}

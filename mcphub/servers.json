{
  "mcpServers": {
    "fetch": {
      "args": ["mcp-server-fetch"],
      "command": "uvx"
    },
    "github": {
      "autoApprove": [
        "get_me",
        "list_pull_requests",
        "get_pull_request",
        "get_pull_request_comments",
        "list_issues",
        "get_issue",
        "get_issue_comments"
      ],
      "headers": {
        "Authorization": "Bearer ${MCP_GITHUB_TOKEN}"
      },
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "memory": {
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "command": "npx"
    },
    "sequentialthinking": {
      "args": ["run", "--rm", "-i", "mcp/sequentialthinking"],
      "command": "docker"
    }
  },
  "nativeMCPServers": {}
}
